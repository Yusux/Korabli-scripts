<root>

	<ClientMethods>

		<receiveNavigationDebugData>
			<Arg> ENTITY_ID </Arg>
			<Arg> NAVIGATION_DEBUG_DATA </Arg>
		</receiveNavigationDebugData>

		<inviteBotToDynamicDivision>
			<Arg>	STRING	</Arg>	<!-- bot name -->
		</inviteBotToDynamicDivision>

		<removeBotFromDynamicDivision>
			<Args>
				<botName>   STRING	</botName>
			</Args>
		</removeBotFromDynamicDivision>

		<createDynamicDivisionOfBots>
			<Args>
				<botList> ARRAY <of> STRING </of> </botList>
			</Args>
		</createDynamicDivisionOfBots>

		<changeDynamicDivisionOwnerToBot>
			<Args>
				<botName>   STRING	</botName>
			</Args>
		</changeDynamicDivisionOwnerToBot>

	</ClientMethods>

	<CellMethods>

		<switchIdealGunHack>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> UINT8 </Arg>
		</switchIdealGunHack>

		<switchIdealATBASelectedGunId>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> UINT8 </Arg>
		</switchIdealATBASelectedGunId>

		<switchCameraShootHack>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> BOOL </Arg>
		</switchCameraShootHack>

		<setFloatCellAppData>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> STRING </Arg> <!-- key -->
			<Arg> FLOAT </Arg> <!-- value -->
		</setFloatCellAppData>

		<setFloatShipParam>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> BLOB </Arg> <!-- parameter path -->
			<Arg> FLOAT </Arg> <!-- value -->
		</setFloatShipParam>

		<bot_spawnSplashAtShootPos>
			<Exposed> OWN_CLIENT </Exposed>
		</bot_spawnSplashAtShootPos>

		<requestNavigationDebugData>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> ENTITY_ID </Arg>
		</requestNavigationDebugData>

		<killEntity>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> TARGET_ID </Arg>
		</killEntity>

        <respawn>
            <Exposed> OWN_CLIENT </Exposed>
            <Args>
                <avatarId> ENTITY_ID </avatarId>
            </Args>
        </respawn>

        <setVehicleMovementBlocked>
			<Exposed> OWN_CLIENT </Exposed>
			<Args>
			    <vehicleId> ENTITY_ID </vehicleId>
			</Args>
		</setVehicleMovementBlocked>

		<setPhysicParams>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> STRING </Arg> <!-- key -->
			<Arg> FLOAT </Arg> <!-- value -->
		</setPhysicParams>

		<damageSquadron>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> INT8 </Arg>
			<Arg> ARRAY <of> UINT8 </of> </Arg>
			<Arg> FLOAT </Arg>
		</damageSquadron>

		<teleportSquadron>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> PLANE_ID </Arg>
			<Arg> VECTOR3 </Arg>
			<Arg> NULLABLE_FLOAT </Arg>
		</teleportSquadron>

		<setSquadronDebugOption>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> UINT16    </Arg>
			<Arg> BOOL      </Arg>
		</setSquadronDebugOption>

		<respawnPlanes>
			<Exposed> OWN_CLIENT </Exposed>
		</respawnPlanes>

		<spawnMinefield>
			<Exposed> OWN_CLIENT </Exposed>
			<Args>
				<position> VECTOR3 </position>
				<paramsId> GAMEPARAMS_ID </paramsId>
			</Args>
		</spawnMinefield>

		<reloadWeapons>
			<Exposed> OWN_CLIENT </Exposed>
		</reloadWeapons>

		<setBlind>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> BOOL </Arg>
		</setBlind>

		<reloadShipParams>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> STRING </Arg>
		</reloadShipParams>

		<enableAllVision>
			<Exposed> OWN_CLIENT </Exposed>
			<Arg> BOOL </Arg>
		</enableAllVision>

	</CellMethods>

	<BaseMethods>

		<earnAchievementInBattle>
			<Arg> UINT32 </Arg>		<!--Battle achievement id-->
			<Flags>	OWN_CLIENT </Flags>
			<Exposed> OWN_CLIENT </Exposed>
		</earnAchievementInBattle>

		<spawnBot>
			<Args>
				<shipName> STRING </shipName>
				<teamId> TEAM_ID </teamId>
				<x> INT16 </x>
				<z> INT16 </z>
				<yaw> INT16 </yaw>
				<aiConfig> UINT16 </aiConfig>
				<botName> STRING </botName>
				<botRoles> ARRAY <of> STRING </of> </botRoles>
				</Args>
			<Exposed> OWN_CLIENT </Exposed>
		</spawnBot>

		<inviteBotToDynamicDivision>
			<Args>
				<botName> STRING </botName>
				<divisionId> PRE_BATTLE_ID </divisionId>
			</Args>
			<Exposed> OWN_CLIENT </Exposed>
		</inviteBotToDynamicDivision>

		<removeBotFromDynamicDivision>
			<Args>
				<botName> STRING </botName>
				<divisionId> PRE_BATTLE_ID </divisionId>
			</Args>
			<Exposed> OWN_CLIENT </Exposed>
		</removeBotFromDynamicDivision>

		<changeDynamicDivisionOwnerToBot>
			<Args>
				<botName> STRING </botName>
				<divisionId> PRE_BATTLE_ID </divisionId>
			</Args>
			<Exposed> OWN_CLIENT </Exposed>
		</changeDynamicDivisionOwnerToBot>

		<fillDynamicDivisionWithBots>
			<Args>
				<botList> ARRAY <of> STRING </of> </botList>
				<divisionId> PRE_BATTLE_ID </divisionId>
				<playersLimit> INT8 </playersLimit>
			</Args>
			<Exposed> OWN_CLIENT </Exposed>
		</fillDynamicDivisionWithBots>

		<sendNewOwnerBotToDynamicDivisionManager>
			<Args>
				<botID> PLAYER_ID </botID>
				<divisionId> PRE_BATTLE_ID </divisionId>
			</Args>
		</sendNewOwnerBotToDynamicDivisionManager>

	</BaseMethods>

	<ofEntity>
		<Avatar/>
	</ofEntity>

	<ofBWComponent>
		<base/>
		<client/>
		<bot/>
		<cell/>
	</ofBWComponent>
</root>
