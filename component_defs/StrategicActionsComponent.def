<root>
	<BaseMethods>
		<selectTerritory>
			<Exposed/>
			<Args>
				<territoryID> STRING </territoryID>
			</Args>
			<ReturnValues>
				<result>BOOL</result>
			</ReturnValues>
		</selectTerritory>
		<claimTerritoryRewards>
			<Exposed/>
			<Args>
				<territoryID> STRING </territoryID>
			</Args>
			<ReturnValues>
				<result>BOOL</result>
			</ReturnValues>
		</claimTerritoryRewards>
		<claimAllRewards>
			<Exposed/>
			<ReturnValues>
				<result>BOOL</result>
			</ReturnValues>
		</claimAllRewards>
	</BaseMethods>

	<ClientMethods>
		<syncEmptyStrategicActions/>
		<onProgressUpdated>
			<Args>
				<territoryID> STRING </territoryID>
				<progress> UINT16 </progress>
				<unlocks> ARRAY <of> STRING </of> </unlocks>
				<autoselectedTerritoryID> STRING </autoselectedTerritoryID>
			</Args>
		</onProgressUpdated>
		<onCurrentTaskChanged>
			<Args>
				<territoryID> STRING </territoryID>
				<currentTaskIndex> UINT8 </currentTaskIndex>
			</Args>
		</onCurrentTaskChanged>
	</ClientMethods>

	<ofEntity>
		<Account/>
	</ofEntity>
	
	<ofBWComponent>
		<base/>
		<bot/>
		<client/>
	</ofBWComponent>
</root>

