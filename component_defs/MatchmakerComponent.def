<root>
	<Properties>
		<_MatchmakerComponent__matchingState>
			<Type> UINT8 </Type>
			<Flags> BASE </Flags>
		</_MatchmakerComponent__matchingState>
		<_MatchmakerComponent__realm>
			<Type> STRING </Type>
			<Flags> BASE </Flags>
		</_MatchmakerComponent__realm>
		<_MatchmakerComponent__queueType>
			<Type> QUEUE_TYPE </Type>
			<Flags> BASE </Flags>
		</_MatchmakerComponent__queueType>
		<_MatchmakerComponent__inDivision>
			<Type> BOOL </Type>
			<Flags> BASE </Flags>
		</_MatchmakerComponent__inDivision>
		<_MatchmakerComponent__battleStarterService>
			<Type> MAILBOX </Type>
			<Flags> BASE </Flags>
		</_MatchmakerComponent__battleStarterService>
	</Properties>

	<BaseMethods>
		<enter>
			<Exposed/>
			<Args>
				<shipID> SHIP_ID </shipID>
				<queueType> QUEUE_TYPE </queueType>
				<mapID> INT16 </mapID>  <!-- MAP_ID (UINT8) or -1 -->
				<gameMode> INT32 </gameMode>  <!-- GAME_MODE (UINT16) or -1 -->
				<operationID> GAMEPARAMS_ID </operationID>
			</Args>
		</enter>

		<enterMatchmakerFromBattle>
			<Args>
				<shipID> SHIP_ID </shipID>
				<queueType> QUEUE_TYPE </queueType>
				<operationID> GAMEPARAMS_ID </operationID>
			</Args>
		</enterMatchmakerFromBattle>

		<leave>
			<Exposed/>
		</leave>

		<onEnterViaDivision>  <!-- Callback from Division -->
			<Args>
				<enqueueData> ENQUEUE_DATA </enqueueData>
			</Args>
		</onEnterViaDivision>

		<onLeave>  <!-- Callback from Division -->
			<Args>
				<reason> UINT8 </reason>
			</Args>
		</onLeave>
	</BaseMethods>

	<ClientMethods>
		<onEnter>
			<Args>
				<queueType> QUEUE_TYPE </queueType>
				<shipID> SHIP_ID </shipID>
				<description> PICKLED_BLOB </description>
			</Args>
		</onEnter>

		<onQueueInfoReceived>
			<Args>
				<queueType> GAMEPARAMS_ID </queueType>  <!-- QUEUE_TYPE (UINT16) or OPERATION_ID (GAMEPARAMS_ID/UINT32) -->
				<queueInfo> PICKLED_BLOB </queueInfo>
			</Args>
		</onQueueInfoReceived>

		<restoreInMatchmakerState>
			<Args>
				<queueType> QUEUE_TYPE </queueType>
			</Args>
		</restoreInMatchmakerState>

		<onLeave>
			<Args>
				<reason> UINT8 </reason>
			</Args>
		</onLeave>
	</ClientMethods>

	<ofEntity>
		<Account/>
	</ofEntity>

	<ofBWComponent>
		<base/>
		<bot/>
		<client/>
	</ofBWComponent>
</root>
