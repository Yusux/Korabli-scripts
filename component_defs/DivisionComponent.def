<root>

	<Properties>
		<_DivisionComponent__divisionMBox>
			<Type> MAILBOX </Type>
			<Flags> BASE </Flags>
		</_DivisionComponent__divisionMBox>
	</Properties>

	<BaseMethods>
		<start>
			<Args>
				<queueType>INT64</queueType>
				<operationID>GAMEPARAMS_ID</operationID>
				<isSoft>INT64</isSoft>
			</Args>
			<Exposed/>
		</start>

		<stop>
			<Args>
				<isSoft>INT64</isSoft>
			</Args>
			<Exposed/>
		</stop>

		<kickPlayer>
			<Args>
				<playerID>INT64</playerID>
			</Args>
			<Exposed/>
		</kickPlayer>

		<setReady>
			<Args>
				<shipID> SHIP_ID </shipID>
				<queueType> QUEUE_TYPE </queueType>
			</Args>
			<Exposed/>
		</setReady>

		<sendInvite>
			<Args>
				<inviteeSPAID> DB_ID </inviteeSPAID>
				<inviteType> INT8 </inviteType>
			</Args>
			<Exposed/>
		</sendInvite>

		<revokeInvite>
			<Args>
				<playerID>INT64</playerID>
			</Args>
			<Exposed/>
		</revokeInvite>

		<quitPreBattle>
			<Exposed/>
		</quitPreBattle>

		<changeOwner>
			<Args>
				<newOwnerID>INT64</newOwnerID>
			</Args>
			<Exposed/>
		</changeOwner>

		<dismiss>
			<Exposed/>
		</dismiss>

		<setSelectedQueueType>
			<Args>
				<selectedQueueType> QUEUE_TYPE </selectedQueueType>
				<gameParamID> GAMEPARAMS_ID </gameParamID>
			</Args>
			<Exposed/>
		</setSelectedQueueType>

		<setIgnore>
			<Exposed/>
		</setIgnore>

		<lock>
			<Exposed/>
		</lock>

		<unlock>
			<Exposed/>
		</unlock>

		<changeGrants>
			<Args>
				<grants>INT64</grants>
			</Args>
		</changeGrants>

		<syncPreBattleDef>
			<Args>
				<packedPreBattleDef>BLOB</packedPreBattleDef>
			</Args>
		</syncPreBattleDef>

		<receiveChatMessage>
			<Args>
				<divisionID> ENTITY_ID </divisionID>
				<senderID> PLAYER_ID </senderID>
				<message> STRING </message>
				<messageCreationTime> TIMESTAMP </messageCreationTime>
			</Args>
		</receiveChatMessage>

		<sendMessageToPlayers>
			<Args>
				<message> STRING </message>
			</Args>
			<Exposed/>
		</sendMessageToPlayers>
	</BaseMethods>

	<ClientMethods>
		<syncPreBattleDef>
			<Args>
				<packedPreBattleDef>BLOB</packedPreBattleDef>
			</Args>
		</syncPreBattleDef>

		<receiveChatMessage>
			<Args>
				<senderID> PLAYER_ID </senderID>
				<message> STRING </message>
				<messageCreationTime> TIMESTAMP </messageCreationTime>
			</Args>
		</receiveChatMessage>
	</ClientMethods>

	<ofEntity>
		<Account/>
	</ofEntity>

	<ofBWComponent>
		<base/>
		<client/>
		<bot/>
	</ofBWComponent>
</root>
